{
  "id": "org.electrosaur.trapper",
  "name": "Trapper",
  "version": "1.0.0",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "23.3.0",
    "data": {
      "apiVersion": 2
    }
  },
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "command",
      "id": "trapper.showDialog",
      "label": {
        "default": "Color Trapping..."
      }
    }
  ],
  "icons": [
    {
      "width": 24,
      "height": 24,
      "path": "icons/icon_light.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ],
      "species": [
        "generic"
      ]
    },
    {
      "width": 24,
      "height": 24,
      "path": "icons/icon_dark.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark"
      ],
      "species": [
        "generic"
      ]
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "plugin",
    "network": {
      "domains": "all"
    },
    "clipboard": "readAndWrite"
  },
  "featureFlags": {
    "enableAlerts": true
  }
}
